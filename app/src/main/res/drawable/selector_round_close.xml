<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- 평상시 -->
    <item android:state_focused="false" android:state_pressed="false">
        <shape>
            <size android:height="@dimen/btn_account_height" />
            <corners android:radius="100dp" />
            <solid android:color="@color/color_00000000" />
            <stroke android:color="@color/color_cccccc" android:width="1dp"/>
        </shape>
    </item>

    <!-- 클릭했을 때 -->
    <item android:state_focused="false" android:state_pressed="true">
        <shape>
            <size android:height="@dimen/btn_account_height" />
            <solid android:color="@color/color_f5f3f3" />
            <corners android:radius="100dp" />
            <stroke android:color="@color/color_555053" android:width="1dp"/>
        </shape>
    </item>


</selector>
